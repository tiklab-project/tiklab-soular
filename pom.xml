<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>io.tiklab</groupId>
        <artifactId>tiklab-dependencies</artifactId>
        <version>1.0.0.3</version>
    </parent>
    <artifactId>tiklab-eas</artifactId>
    <packaging>pom</packaging>
    <name>${project.artifactId}</name>
    <version>1.0.0.3</version>

    <modules>
        <module>tiklab-eas-api</module>
        <module>tiklab-eas-server</module>
        <module>tiklab-eas-starter</module>
    </modules>

    <properties>
        <tiklab-eas.version>${version}</tiklab-eas.version>
        <!--tiklab-->
        <tiklab-dal.version>1.0.0.3</tiklab-dal.version>
        <tiklab-core.version>1.0.0.3</tiklab-core.version>
        <tiklab-toolkit.version>1.0.0.3</tiklab-toolkit.version>
        <tiklab-rpc.version>1.0.0.3</tiklab-rpc.version>
        <tiklab-dss.version>1.0.0.3</tiklab-dss.version>
        <tiklab-dcs.version>1.0.0.3</tiklab-dcs.version>
        <tiklab-dfs.version>1.0.0.3</tiklab-dfs.version>
        <tiklab-codegen.version>1.0.0.3</tiklab-codegen.version>
        <tiklab-gateway.version>1.0.0.3</tiklab-gateway.version>
        <tiklab-plugin.version>1.0.0.3</tiklab-plugin.version>

        <tiklab-user.version>1.0.0.3</tiklab-user.version>
        <tiklab-eam.version>1.0.0.3</tiklab-eam.version>
        <tiklab-message.version>1.0.0.3</tiklab-message.version>
        <tiklab-security.version>1.0.0.3</tiklab-security.version>
        <tiklab-todotask.version>1.0.0.3</tiklab-todotask.version>
        <tiklab-licence.version>1.0.0.3</tiklab-licence.version>
        <tiklab-privilege.version>1.0.0.3</tiklab-privilege.version>

        <tiklab-postin.version>1.0.0.3</tiklab-postin.version>
        <tiklab-postgresql.version>1.0.0.3</tiklab-postgresql.version>
        <tiklab-jdk.version>1.0.0.3</tiklab-jdk.version>
        <tiklab-upgrade.version>1.0.0.3</tiklab-upgrade.version>
    </properties>


    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-core</artifactId>
                <version>${tiklab-core.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-toolkit-annotation</artifactId>
                <version>${tiklab-toolkit.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-dal-spring-boot-starter</artifactId>
                <version>${tiklab-dal.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-rpc-spring-boot-starter</artifactId>
                <version>${tiklab-rpc.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-dcs-spring-boot-starter</artifactId>
                <version>${tiklab-dcs.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-dss-annotation</artifactId>
                <version>${tiklab-dss.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-dss-spring-boot-starter</artifactId>
                <version>${tiklab-dss.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-codegen-core</artifactId>
                <version>${tiklab-codegen.version}</version>
            </dependency>
            <dependency>
                <groupId>io.tiklab</groupId>
                <artifactId>tiklab-postin-annotation</artifactId>
                <version>${tiklab-postin.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
            </plugin>
        </plugins>
    </build>
</project>

